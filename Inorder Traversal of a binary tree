// Recursive Approach

void inorder(TreeNode*root,vector<int>&v){
        if(root==NULL) return ;
        inorder(root->left,v);
        v.push_back(root->val);
        inorder(root->right,v);
        return;
    }
    vector<int> inorderTraversal(TreeNode* root) {
        if(root==NULL   ){
            vector<int>v;
            return v;
        }
        vector<int>v;
        inorder(root,v);
        return v;
    }
